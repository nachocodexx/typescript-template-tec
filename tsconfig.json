        "outDir": "./dist",
        "lib": [
            "es6"
        ],
        "allowJs": true,
        "target": "es6",
        "module": "commonjs",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
    },
    "exclude": [
        "./node_modules",
    ]
}